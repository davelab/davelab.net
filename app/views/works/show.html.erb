<p id="notice"><%= notice %></p>

<p>
  <b>Category:</b>
  <%= @work.category_id %>
</p>

<p>
  <b>Name:</b>
  <%= @work.name %>
</p>

<p>
  <b>Description:</b>
  <%= @work.description %>
</p>
<%- @work.images.each do |image| %>
  <%= image_tag image.url(:small) %>
<% end %>
<p>
  <b>Link:</b>
  <%= @work.link %>
</p>

<p>
  <b>Realized at:</b>
  <%= @work.realized_at %>
</p>


<%= link_to 'Edit', edit_work_path(@work) %> |
<%= link_to 'Back', works_path %>
